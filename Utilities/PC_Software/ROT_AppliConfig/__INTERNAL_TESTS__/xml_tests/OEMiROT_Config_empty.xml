<?xml version='1.0' encoding='UTF-8'?>
<Root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SCHVerif.xsd">
<Obdata>
	<Info>
		<Title>OEMiROT configuration</Title>		
		<ObDestAddress>0x0FFD0160</ObDestAddress>
		<DoEncryption>1</DoEncryption>
		<GlobalAlign>16</GlobalAlign>
		<FlashStart>0x0C000000</FlashStart>
		<FlashSize>0x200000</FlashSize>
		<FlashSectorSize>0x2000</FlashSectorSize>
		<OBKStart>0xFFD0900</OBKStart>
		<OBKSize>0x2F0</OBKSize>
		<OBKSectorSize>0x10</OBKSectorSize>
		<AuthOBKStart>0xFFD0900</AuthOBKStart>
		<AuthOBKSize>0x2F0</AuthOBKSize>
	</Info>
	<Hash>
	</Hash>
	<File>
		<Name>Authentication secure key</Name>
		<Value>output.pem</Value>
		<Align>4</Align>
		<KeyType></KeyType>
		<Type>Public</Type>
		<Default>OEMiRoT_Authentication_S.pem</Default>
		<Tooltip>Select the key used to authenticate secure image. When this key is regenerated, image must be processed with Imgtool</Tooltip>
	</File>
	<File>
		<Name>Authentication non secure key</Name>
		<Value>output.pem</Value>
		<Align>4</Align>
		<KeyType></KeyType>
		<Type>Public</Type>
		<Default>OEMiRoT_Authentication_NS.pem</Default>
		<Tooltip>Select the key used to authenticate non secure image. When this key is regenerated, image must be processed with Imgtool</Tooltip>
	</File>
	<File>
		<Name>Hdpl3EncryptionPrivKey</Name>
		<Value>OEMiRoT_Encryption_Priv.pem</Value>
		<Align>4</Align>
		<KeyType></KeyType>
		<Type></Type>
		<Default>OEMiRoT_Encryption_Priv.pem</Default>
		<Tooltip>Select the key used to encrypt images</Tooltip>
	</File>
	<Output>
		<Name>Output File</Name>
		<Value>OEMiROT_Config.obk</Value>
		<Default>OEMiRoT_Config.obk</Default>
		<Tooltip>Select the output file .obk including OEMiROT configuration</Tooltip>
	</Output>
</Obdata>
</Root>